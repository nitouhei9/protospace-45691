<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは<%= link_to current_user.name, user_path(current_user) %>さん
      </div>
    <% end %>
    
    <div class="card__wrapper">
      <%# collectionオプションを使用して部分テンプレートを呼び出す %>
      <%= render partial: "prototype", collection: @prototypes %>
    </div>
  </div>
</main>